<h1 class="centered">Hanami Contributors</h2>

<div id="masonry-grid">
  <div class="gutter-sizer"></div>
  <% contributors.each do |contributor| %>
  <div class="grid-item">
    <%= avatar(contributor) %>
  </div>
  <% end %>
</div>

<div class="centered">
  <%= image "hanami.png", width: "96px", height: "96px", alt: "Application Logo", push: :image %>
</div>
